{
  "name": "electron-react-ant-boilerplate",
  "version": "1.0.9",
  "description": "Electron, Webpack4, React, Babel 7, ES6, PostCSS, Sass, Ant Design",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevoj/electron-react-ant-boilerplate.git"
  },
  "author": {
    "name": "leonardo R",
    "email": "leonardo.ricog@gmail.com",
    "url": "https://github.com/kevoj"
  },
  "keywords": [
    "react",
    "electron",
    "boilerplate",
    "webpack",
    "react-electron",
    "react-electron-webpack",
    "electron-boilerplate",
    "react-boilerplate",
    "react-ant-boilerplate",
    "react-ant",
    "ant-boilerplate",
    "ant-design",
    "react-ant-design",
    "react-ant-design-boilerplate",
    "react-webpack",
    "ant-webpack",
    "react-babel",
    "webpack",
    "es6",
    "babel-7",
    "less",
    "sass"
  ],
  "engines": {
    "node": ">=8.0.0 <12",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "homepage": "",
  "main": "main.js",
  "scripts": {
    "start": "npm run env:dev -- webpack-dev-server",
    "build": "npm run env:prod -- webpack",
    "prod": "npm run build && electron --noDevServer .",
    "package": "npm run build",
    "package:all": "npm run build",
    "test": "jest -u",
    "lint": "eslint ./src",
    "docs": "jsdoc -r ./src -d docs --verbose",
    "env:dev": "cross-env NODE_ENV=development",
    "env:prod": "cross-env NODE_ENV=production",
    "build:clean": "rimraf -rf dist",
    "build:copy": "copyfiles -u 1 public/* public/**/* dist -e public/index.html",
    "prebuild": "npm run build:clean && npm run build:copy",
    "prepackage:all": "rimraf -rf release",
    "postpackage": "electron-builder build",
    "postpackage:all": "electron-builder build -mwl"
  },
  "dependencies": {
    "antd": "^3.20.3",
    "classnames": "^2.2.6",
    "electron-is-dev": "^1.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@hot-loader/react-dom": "^16.8.6",
    "antd-scss-theme-plugin": "^1.0.7",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.0",
    "babel-plugin-root-import": "^6.4.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babili-webpack-plugin": "^0.1.2",
    "brotli-webpack-plugin": "^1.1.0",
    "copyfiles": "^2.1.1",
    "cross-env": "^5.2.0",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^3.1.0",
    "electron": "^5.0.7",
    "electron-builder": "^21.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^6.0.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-react": "^7.14.2",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "jsdoc": "^3.6.3",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-pxtorem": "^4.0.1",
    "react-hot-loader": "^4.12.8",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "thread-loader": "^2.1.2",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "build": {
    "productName": "MyApp",
    "appId": "org.develar.MyApp",
    "files": [
      "dist",
      "main.js",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/@test/config/setupTests.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "babel-jest",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/@test/config/fileMock.js",
      "^components$": "<rootDir>/@test/config/componentsMock.js"
    },
    "moduleDirectories": [
      "src",
      "@test/src",
      "node_modules"
    ]
  }
}
